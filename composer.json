{
  "name": "tourze/backtrace-helper",
  "description": "Backtrace helper",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "spatie/backtrace": "^1.7.1",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\BacktraceHelper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\BacktraceHelper\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  }
}